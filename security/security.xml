<odoo>
    <record id="todo_category" model="ir.module.category">
        <field name="name">Todo</field>
    </record>
    <record id="todo_manager" model="res.groups">
        <field name="name">Todo Manager</field>
        <field name="category_id" ref="TaskOne.todo_category"/>
    </record>
    <record id="todo_user" model="res.groups">
        <field name="name">Todo User</field>
        <field name="category_id" ref="TaskOne.todo_category"/>
    </record>
    <record id="todo_by_user" model="ir.rule">
        <field name="name">Todo.rule.by.user</field>
        <field name="model_id" ref="TaskOne.model_todo"/>
        <field name="domain_force">[('create_uid','=',user.id)]</field>
    </record>
</odoo>